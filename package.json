{
    "name": "qooco-apps",
    "version": "1.1.2",
    "license": "MIT",
    "scripts": {
        "start": "concurrently \"ng serve --host 0.0.0.0 --proxy-config proxy.conf.json\" \"nodemon --watch server server.js\"",
        "start:prod": "pm2 start pm2.config.json",
        "start:staging": "pm2 start pm2.config.json",
        "start:heroku": "cross-env NODE_ENV=heroku node server.js",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "build:prod": "ng build --aot --prod --base-href /apps/",
        "build:heroku": "ng build --aot --prod",
        "postinstall": "npm run build:heroku"
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/build-angular": "~0.6.0",
        "@angular/animations": "6.0.0",
        "@angular/cli": "^6.0.0",
